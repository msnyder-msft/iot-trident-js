{
    "name": "@microsoft/iot-trident-js",
    "version": "0.0.0-development",
    "description": "",
    "main": "index.js",
    "types": "index.d.ts",
    "module": "index.js",
    "sideEffects": [
        "**/*.css",
        "**/*.scss"
    ],
    "scripts": {
        "build": "npm run clean && node -max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c rollup.config.js && npm run post-build",
        "chromatic": "chromatic --project-token CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
        "clean": "shx rm -rf dist && shx rm -rf build_artifacts",
        "copy-package": "shx cp package.json dist",
        "copy-readme": "shx cp README.md dist/",
        "copy-theme-sheet": "node-sass src/Resources/Styles/BaseThemeVars.scss dist/themes.css",
        "eslint:check": "eslint src/**/*.{js,ts,tsx} -c ./.eslintrc.prod.json",
        "eslint:fix": "eslint src/**/*.{js,ts,tsx} --cache --fix",
        "generate-schema": "node .\\tools\\generate-3d-schema-types.js",
        "generate": "node ./tools/generate-component-template.js",
        "generateCerts": "mkdirp certificates && cd certificates && mkcert create-ca && mkcert create-cert",
        "lint:check": "npm run eslint:check && npm run prettier:check && npm run stylelint:check",
        "lint": "npm run prettier:fix && npm run stylelint:fix",
        "pack": "shx mkdir ./build_artifacts && cd dist && npm pack && shx mv *.tgz ../build_artifacts",
        "post-build": "npm run copy-theme-sheet && npm run copy-package && npm run copy-readme && npm run pack",
        "prettier:check": "prettier --check src/**/*.{js,ts,tsx,scss}",
        "prettier:fix": "prettier --write src/**/*.{js,ts,tsx,scss}",
        "storybook:export": "build-storybook",
        "storybook:noSSL": "start-storybook -p 3001",
        "storybook": "npm run generateCerts && start-storybook -p 443 --https --ssl-ca certificates/ca.crt --ssl-cert certificates/cert.crt --ssl-key certificates/cert.key",
        "stylelint:check": "stylelint \"src/**/*.scss\"",
        "stylelint:fix": "stylelint --fix \"src/**/*.scss\"",
        "test:ci": "node --max_old_space_size=8192 node_modules/.bin/jest --config jest.config.js --maxWorkers=2",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
        "test:watch": "jest --watch",
        "test": "jest"
    },
    "dependencies": {
        "@babel/runtime": "^7.7.2",
        "@fluentui/react": "^8.67.2",
        "@trident/extension-client": "^1.24.4",
        "axios": "^0.21.1",
        "axios-retry": "^3.1.9",

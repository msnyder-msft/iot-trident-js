        "react-inlinesvg": "^3.0.0",
        "react-transition-group": "^4.4.2",
        "ts-md5": "^1.3.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.10",

        "react-router-dom": "^5.3.0",
        "rollup": "^2.71.1",
        "rollup-plugin-dts": "^4.1.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-rename-node-modules": "^1.2.0",
        "rollup-plugin-typescript2": "^0.31.1",
        "sass-loader": "^10.1.0",
        "seedrandom": "^3.0.5",
